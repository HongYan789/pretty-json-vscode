{
  "name": "pretty-json",
  "displayName": "Pretty JSON",
  "description": "this is pretty json plugin tools",
  "version": "0.0.1",
  "publisher": "HongYan",
  "icon": "pretty-json.png",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "pretty-json-vscode.PrettyJSON",
      "title": "Pretty JSON"
    },
    {
      "command": "pretty-json-vscode.IndentedJSON",
      "title": "Indented JSON"
    }],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection && resourceLangId == json",
          "command": "pretty-json-vscode.PrettyJSON",
          "group": "2_format"
        },
        {
          "when": "editorHasSelection && resourceLangId == json",
          "command": "pretty-json-vscode.IndentedJSON",
          "group": "2_format"
        }
      ]
    },
    "keybindings": [
        {
            "command": "pretty-json-vscode.PrettyJSON",
            "key": "cmd+j",
            "mac": "cmd+j",
            "when": "editorTextFocus"
        },
        {
            "command": "pretty-json-vscode.IndentedJSON",
            "key": "cmd+ctrl+j",
            "mac": "cmd+ctrl+j",
            "when": "editorTextFocus"
        }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
